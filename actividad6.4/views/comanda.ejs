<%- include("header") %>
<form action="/comanda" method="post">
    <label for="usuario_id">usuario_id </label>
    <select name="usuario_id" id="usuario_id" class="form-select" aria-label="Default select example">
        <% usuarios.forEach(function(usuario) { %>
            <option value="<%= usuario.id %>"><%= usuario.nombre %></option>
        <% }); %>
    </select><br>
    <label for="producto_id">producto_id </label>
    <select name="producto_id" id="producto_id" class="form-select" aria-label="Default select example">
        <% productos.forEach(function(producto) { %>
            <option value="<%= producto.id %>"><%= producto.nombre %></option>
        <% }); %>
    </select><br>
    <button type="submit">Enviar</button>
</form>
<%- include("footer") %>